package com.wordpress.data;

import org.testng.annotations.DataProvider;

public class wordpressData {
	
	@DataProvider
	public void validUsernamePasswordProvider(){
		//variables for input parameters
		String[] UserPass = new String[2];
		UserPass[0]="chandra.lipika.test@gmail.com";
		UserPass[0]="Test@1234";
	
	}
	
	@DataProvider
	public void invalidUsernamePasswordProvider(){
		//variables for input parameters
		String[] UserPass = new String[2];
		UserPass[0]="chandrass.lipika.test@gmail.com";
		UserPass[0]="Tesaat@1234";
	
	}

}
